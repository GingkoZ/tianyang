<table class="table table-striped table-hover" id="learn-data-table" style="word-break:break-all;">
	<thead>
		<tr>
		<th>编号</th>
		<th width="30%">姓名</th>
		<th>手机号</th>
		<th>用户名</th>
		<th>所属组织</th>
		<th>邮箱</th>
		<th>学习总时长</th>
		<th>听课率</th>
		</tr>
	</thead>
	<tbody>
	{% for itme in list %}
		<tr id="user-learn-data-tr-{{ itme.id }}">
			<td>{{ itme.id }}</td>
			<td>{{ itme.truename }}</td>
			<td>{{ itme.mobile}}</td>
			<td>{{ itme.nickname}}</td>
			<td>{{ itme.organizationFullPath}}</td>
			<td>{{ itme.email}}</td>
			<td>{{ itme.learnTime}}</td>
			<td>{{ itme.learnLessonRate}}</td>
		</tr>

	{% else %}
		<tr>
			<td colspan="20"><div class="empty">暂无课程记录</div></td>
		</tr>
	{% endfor %}
	</tbody>
</table>